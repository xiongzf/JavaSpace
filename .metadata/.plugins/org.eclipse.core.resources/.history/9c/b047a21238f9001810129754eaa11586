package com.xzf.domain;

import java.util.List;

/**
 * 这是一个用于封装了分页的数据.
 * 里面包含
 * 			该页的学生数据集合
 * 			总的记录数
 * 			总的页数
 * 			当前页
 * 			每页显示的记录数
 * @author commafit
 *
 */
public class PageBean {
	private int currentPage;
	private int totalPage;
	private int pageSize;
	private int totalSize;
	private List<Student> list;
}
